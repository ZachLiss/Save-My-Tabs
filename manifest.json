{
	"manifest_version": 2,

	"name": "Save my Tabs",
	"version": "1.0.0",

	"permissions": [
		"bookmarks",
		"tabs"],

	"browser_action": {
		"default_title": "Save my Tabs",
		"default_icon": "icons/icon48.png",
		"default_popup": "index.html"
	},

	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}