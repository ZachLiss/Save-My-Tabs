{
	"manifest_version": 2,

	"name": "Bookmark My Tabs",
	"version": "1.0.0",
	"options_page": "options.html",

	"permissions": [
		"bookmarks",
		"tabs"
	],

	"browser_action": {
		"default_title": "Bookmark My Tabs",
		"default_icon": "icons/icon48.png",
		"default_popup": "index.html"
	},

	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}
